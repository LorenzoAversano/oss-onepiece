    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lorenzo\\OssOnepiece\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Lorenzo",
            "email": "lorenzo.aversano@edu.devinci.fr"
        }
